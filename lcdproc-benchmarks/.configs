LCDd_minconf_master=$(
	cat <<-'EOF'
		[server]
		DriverPath=/usr/local/lib/lcdproc/
		Driver=text
	EOF
)

LCDd_minconf_lowlevel=$(
	cat <<-'EOF'
		kdb set 'user/sw/lcdproc/lcdd/#0/current/server/driverpath' '/usr/local/lib/lcdproc/'
		kdb set 'user/sw/lcdproc/lcdd/#0/current/server/driver/#0' '@/text'
		kdb setmeta 'user/sw/lcdproc/lcdd/#0/current/server/driver' array '#0'
	EOF
)

LCDd_minconf_highlevel=$(
	cat <<-'EOF'
		kdb set 'user/sw/lcdproc/lcdd/#0/current/server/drivers/#0' '@/text/#0'
	EOF
)

LCDd_smallconf_master=$(
	cat <<-'EOF'
		[server]
		DriverPath=/usr/local/lib/lcdproc/
		Driver=curses
		Port=9123
		
		[menu]
		MenuKey=Escape
		EnterKey=Enter
		UpKey=Up
		DownKey=Down
		
		[curses]
		Foreground=green
		Background=red
		Backlight=black
		Size=40x10
		TopLeftX=3
		TopLeftY=3
	EOF
)

LCDd_smallconf_lowlevel=$(
	cat <<-'EOF'
		kdb rm -r user/sw/lcdproc/lcdd/#0/current
		
		kdb_set() {
			kdb set -N user "$1" "$2"
			kdb rmmeta "$1" origvalue
			kdb lsmeta "$1" | grep '^internal/' | xargs -L1 kdb rmmeta "user$1"
		}
		
		kdb_set '/sw/lcdproc/lcdd/#0/current/server/driverpath' '/usr/local/lib/lcdproc/'
		kdb_set '/sw/lcdproc/lcdd/#0/current/server/driver/#0' '@/curses'
		kdb setmeta 'user/sw/lcdproc/lcdd/#0/current/server/driver' array '#0'
		kdb_set '/sw/lcdproc/lcdd/#0/current/server/port' '9123'
		
		kdb_set '/sw/lcdproc/lcdd/#0/current/menu/menukey' 'Escape'
		kdb_set '/sw/lcdproc/lcdd/#0/current/menu/enterkey' 'Enter'
		kdb_set '/sw/lcdproc/lcdd/#0/current/menu/upkey' 'Up'
		kdb_set '/sw/lcdproc/lcdd/#0/current/menu/downkey' 'Down'
		
		kdb_set '/sw/lcdproc/lcdd/#0/current/curses/foreground' 'green'
		kdb_set '/sw/lcdproc/lcdd/#0/current/curses/background' 'red'
		kdb_set '/sw/lcdproc/lcdd/#0/current/curses/backlight' 'black'
		kdb_set '/sw/lcdproc/lcdd/#0/current/curses/size' '40x10'
		kdb_set '/sw/lcdproc/lcdd/#0/current/curses/topleftx' '3'
		kdb_set '/sw/lcdproc/lcdd/#0/current/curses/toplefty' '3'
	EOF
)

