		Size=20x4
		Speed=19200
		Reboot=no
		Keypad=yes
		keypad_test_mode=yes
		
		[glk]
		Device=/dev/lcd
		Contrast=500
		Speed=19200
		
		[hd44780]
		ConnectionType=4bit
		Model = standard
		Port=0x378
		Device=/dev/ttyS0
		Speed=0
		Keypad=no
		Contrast=0
		Brightness=1000
		OffBrightness=0
