		Lastline=yes
		Size=20x4
		vspan=2,2
		ExtendedMode=yes
		LineAddress=0x10
		CharMap=hd44780_default
		FontBank=0
		DelayMult=2
		KeepAliveDisplay=0
		RefreshDisplay=5
		DelayBus=true
		KeyMatrix_4_1=Enter
		KeyMatrix_4_2=Up
		KeyMatrix_4_3=Down
		KeyMatrix_4_4=Escape
		
		[lb216]
		Device=/dev/lcd
		Brightness=255
		Speed=9600
		Reboot=no
		
		[lcdm001]
		Device=/dev/ttyS1
		PauseKey=LeftKey
		BackKey=UpKey
		ForwardKey=DownKey
		MainMenuKey=RightKey
		
		[linux_input]
		key=1,Escape
		key=28,Enter
		key=96,Enter
		key=105,Left
		key=106,Right
		key=103,Up
		key=108,Down
		
		[MtxOrb]
		Device=/dev/ttyS0
		Size=20x4
		Type=lkd
		Contrast=480
		hasAdjustableBacklight=no
		Brightness=1000
		OffBrightness=0
		Speed=19200
		KeyMap_A=Left
		KeyMap_B=Right
		KeyMap_C=Up
		KeyMap_D=Down
		KeyMap_E=Enter
		KeyMap_F=Escape
		keypad_test_mode=no
		
		[pyramid]
		Device=/dev/ttyUSB0
		
		[text]
		Size=20x4
		
		[xosd]
		Size=20x4
		Offset=200x200
		Font=-*-clean-*-r-*-*-*-160-*-*-*-*-*
	EOF
)

